doc:
  image: docker.onedata.org/onedata-documentation:
  volumes:
    - /usr/share/nginx/html
nginx:
  container_name: nginx
  restart: always
  image: nginx
  hostname: docs-onedata.cloud.plgrid.pl
  volumes:
    - /etc/localtime:/etc/localtime:ro
  volumes_from:
    - doc
  links:
    - doc  
  ports:
    - 80:80
